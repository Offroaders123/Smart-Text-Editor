{
  "compilerOptions": {
    "module": "NodeNext",
    "target": "ESNext",
    "isolatedModules": true,
    "jsx": "react-jsx",
    "types": [
      "@stedit/menu-drop",
      "@stedit/num-text",
      "new-javascript",
      "vite/client"
    ],
    "noEmit": true,
    "skipLibCheck": true,
    "strict": true,
    "noImplicitOverride": true,
    "noPropertyAccessFromIndexSignature": true,
    "noUncheckedIndexedAccess": true,
    "allowUnreachableCode": false,
    "noUnusedLocals": true,
    "noUnusedParameters": true
  }
}